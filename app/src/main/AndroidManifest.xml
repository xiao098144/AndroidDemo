<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.xiao.demo.recyclerview">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <!-- To access Google+ APIs: -->
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".ui.activity.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ui.activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <!-- Image Selector Entry -->
        <activity
            android:name="me.nereo.multi_image_selector.MultiImageSelectorActivity"
            android:configChanges="orientation|screenSize" />
        <activity
            android:name=".ui.activity.RecyclerViewActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ui.activity.RoundProgressActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".picture.PicassoActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ui.activity.WebviewActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ui.activity.FlexBoxActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ui.activity.WheelViewActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ui.activity.DataBindingActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".design.DesignActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".design.CardViewActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".picture.PicassoListActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".picture.GlideListActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".picture.WatchPictureActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".picture.FrescoActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".rxjava.RxjavaActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".design.NestedScrollActivity"
            android:screenOrientation="portrait"
            android:theme="@style/AppThemeNoActionBar" />
        <activity
            android:name=".design.ToolBarActivity"
            android:theme="@style/AppThemeNoActionBar" />
        <activity
            android:name=".design.CollapsingToolBarLayoutActivity"
            android:theme="@style/AppTheme" />
        <activity android:name=".ui.activity.GestureListenerActivity" />
        <activity android:name=".test.TestActivity" />
        <activity android:name=".netutil.okhttp.OkHttpActivity">
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />

                <data
                    android:host="qqq"
                    android:path="/OKHttp"
                    android:pathPrefix="/okhttptest"
                    android:scheme="http" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.activity.BaseActivity" />
        <activity android:name=".animation.AnimationActivity" />
        <activity android:name=".widget.customView.BezierLineActivity" />
        <activity
            android:name=".ndk.NDKActivity"
            android:label="@string/title_activity_ndk"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ui.activity.DrawerLayoutActivity"
            android:screenOrientation="landscape"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".test.TestActivity2" />
        <activity android:name=".test.JSTestActivity" />
        <activity
            android:name=".ui.activity.FragmentHostActivity"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name=".ui.activity.DynamicTabActivity"
            android:screenOrientation="portrait"
            android:theme="@style/ColorTranslucentTheme"></activity>

        <activity
            android:name=".widget.searchview.SearchActivity"
            android:launchMode="singleTop"
            android:screenOrientation="portrait">

        </activity>

        <activity
            android:name=".widget.searchview.SearchResultActivity"
            android:launchMode="singleTop"
            android:screenOrientation="portrait">

        </activity>

        <activity
            android:name="com.xiao.demo.recyclerview.ui.activity.StickyRecyclerViewActivity"
            android:launchMode="singleTop"
            android:screenOrientation="portrait"></activity>

        <activity
            android:name=".ui.activity.GridViewActivity"
            android:launchMode="singleTop"
            android:screenOrientation="portrait">

        </activity>

        <activity
            android:name=".widget.customView.IrreguralClickActivity"
            android:launchMode="singleTop"
            android:screenOrientation="portrait"></activity>

        <activity android:name=".widget.customView.IrreguralClickActivity2" />

        <activity android:name=".lazyload.LazyLoadActivity" />

        <activity
            android:name=".architectture.mvp.MVPActivity"
            android:launchMode="singleTop"
            android:screenOrientation="portrait" />

        <activity android:name=".animation.AnimationTestActivity" />

    </application>

</manifest>